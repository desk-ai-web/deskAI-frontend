{
  "rewrites": [
    {
      "source": "/((?!assets|icon|favicon|.*\\.(png|jpg|jpeg|gif|ico|svg|webp)).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/icon_with_background.png",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/icon.ico",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
